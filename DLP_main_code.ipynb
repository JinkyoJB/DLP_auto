   "outputs": [],
   "source": [
    "def readUntilExitCode(ser, exitcode=b'*'):\n",
    "    readed = b''\n",
    "    while True:\n",
    "        data = ser.read()\n",
    "        print(data)\n",
    "        readed += data\n",
    "        if exitcode in data:\n",
    "            return readed[:1]"
   ]
  },
